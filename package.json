{
  "name": "fuzzysort-esm",
  "version": "3.0.1",
  "author": "hlysine",
  "license": "MIT",
  "description": "Fast SublimeText-like fuzzy search for JavaScript. ESM fork of farzher/fuzzysort.",
  "keywords": [
    "fuzzy search",
    "fuzzy",
    "search",
    "filter",
    "javascript",
    "node",
    "fast",
    "sublime"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hlysine/fuzzysort.git"
  },
  "main": "fuzzysort.ejs",
  "scripts": {
    "minify": "uglifyjs fuzzysort.ejs -o fuzzysort.min.ejs -m -c --mangle-props keep_quoted --comments /farzher/"
  },
  "files": [
    "fuzzysort.ejs",
    "fuzzysort.min.ejs",
    "index.d.ts"
  ],
  "types": "index.d.ts",
  "devDependencies": {
    "uglify-js": "^3.17.4"
  }
}
